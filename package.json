{
  "name": "bend",
  "displayName": "Bend",
  "description": "A syntax highlighter for bend programming language",
  "icon": "./icons/icon.png",
  "version": "0.0.7",
  "license": "MIT",
  "author": "Rohan Vashisht",
  "repository": "https://github.com/RohanVashisht1234/bend-syntax-highlighter",
  "publisher": "RohanVashisht",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages",
    "Debuggers",
    "Snippets"
  ],
  "main": "./src/extension.js",
  "activationEvents": [],
  "contributes": {
    "languages": [
      {
        "id": "bend",
        "aliases": [
          "Bend",
          "bend"
        ],
        "extensions": [
          ".bend"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "views": {
      "myCustomContainer": [
        {
          "id": "myCustomView",
          "name": "View Run/Debug/Generate options"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "myCustomContainer",
          "title": "Bend",
          "icon": "./icons/sideBarIcon.png"
        }
      ]
    },
    "grammars": [
      {
        "language": "bend",
        "scopeName": "source.bend",
        "path": "./syntaxes/bend.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "bend",
        "path": "./snippets/bend.json"
      }
    ],
    "commands": [
      {
        "command": "runBendFile",
        "title": "runBendFile",
        "shortTitle": "runBendFile",
        "icon": "$(play)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "runBendFile",
          "when": "editorLangId == bend",
          "group": "navigation"
        }
      ]
    },
    "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keybindings": [
      {
        "command": "runBendFile",
        "key": "F5"
      }
    ]
  }
}